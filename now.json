{
  "version": 2,
  "name": "gatsby-theme-moltin",
  "alias": "gatsby-theme-moltin",
  "build": {
    "env": {
      "MOLTIN_CLIENT_ID": "@gatsby-theme-moltin-moltin-client-id"
    }
  },
  "builds": [
    {
      "src": "example/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/example/$1"
    }
  ]
}
